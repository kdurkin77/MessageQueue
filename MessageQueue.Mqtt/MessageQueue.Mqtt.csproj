<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net6;net7;</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <Authors>kdurkin77;reed-adams;</Authors>
    <PackageId>KM.MessageQueue.Mqtt</PackageId>
    <PackageProjectUrl>https://github.com/kdurkin77/MessageQueue</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/kdurkin77/MessageQueue</RepositoryUrl>
    <PackageTags>MessageQueue;Queue;Mqtt;</PackageTags>
    <Product>KM.MessageQueue.Mqtt</Product>
    <Description>An MQTT implementation from the interfaces in KM.MessageQueue</Description>
    <PackageReleaseNotes>
        Ensuring connection on creation of queue and subscription on creation of reader
        Using the original client from the queue to create a new manager in the reader
        Updated packages and using packages instead of project references
    </PackageReleaseNotes>
    <Version>7.2.1</Version>
  </PropertyGroup>

  <PropertyGroup>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <RootNamespace>KM.MessageQueue.Mqtt</RootNamespace>
    <AssemblyName>KM.MessageQueue.Mqtt</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="KM.MessageQueue" Version="7.1.0" />
    <PackageReference Include="KM.MessageQueue.Formatters.ObjectToJsonString" Version="7.1.0" />
    <PackageReference Include="KM.MessageQueue.Formatters.Specialized.Composition" Version="7.1.0" />
    <PackageReference Include="KM.MessageQueue.Formatters.StringToBytes" Version="7.1.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.1" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="7.0.1" />
    <PackageReference Include="MQTTnet.Extensions.ManagedClient" Version="4.3.1.873" />
    <PackageReference Include="System.Threading.Channels" Version="7.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
